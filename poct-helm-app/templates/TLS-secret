apiVersion: bitnami.com/v1alpha1
kind: SealedSecret
metadata:
  name: my-tls-secret
spec:
  encryptedData:
    tls.crt: {{ .Values.sealedSecrets.tlsCrt | quote }}
    tls.key: {{ .Values.sealedSecrets.tlsKey | quote }}
  template:
    metadata:
      name: my-tls-secret
    type: kubernetes.io/tls


